<div class="title">
  <h1>つぶやきを編集する</h1>
</div>

<div class="tweet_panel">
  <%= form_with(model: @twitter, local: true) do |form|%>
  <div class="tweet">
    <%= form.label :content%>
    <%= form.text_field :content%>
  </div>
  <div class="submit">
  <%= form.submit '編集する'%>
  </div>
  <% end%>
</div>
